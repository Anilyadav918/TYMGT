{
  "name": "nodejs_http",
  "version": "1.0.0",
  "description": "A nodejs backend for making https requests on dummy data",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "json-server": "json-server --watch db.json --port 5000",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run json-server\""
  },
